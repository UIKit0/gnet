CC = gcc
FLAGS = -g -Wall -mno-cygwin -mpentium -fnative-struct -DGNET_EXPERIMENTAL=1
LINK =  -lgnet-1.1 -lglib-1.3 -lws2_32
INC = -I../glib -I. -I../gnet -L../glib -L../gnet
all:
	$(CC) $(FLAGS) $(INC) hostinfo.c -o hostinfo $(LINK)
	$(CC) $(FLAGS) $(INC) dnstest.c -o dnstest $(LINK)
	$(CC) $(FLAGS) $(INC) pack.c -o pack $(LINK)
	$(CC) $(FLAGS) $(INC) unpack.c -o unpack $(LINK)